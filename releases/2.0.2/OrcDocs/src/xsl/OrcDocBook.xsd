<!-- DocBook V5.0-Based Extension -->

<xs:schema targetNamespace="http://orc.csres.utexas.edu/OrcDocBook.xsd" xmlns="http://orc.csres.utexas.edu/OrcDocBook.xsd" xmlns:db="http://docbook.org/ns/docbook" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:element name="sitepropset">
	    <xs:complexType>
	    	<xs:sequence>
	    		<xs:element ref="siteprop"/>
	    	</xs:sequence>
	    </xs:complexType>
    </xs:element>
    
    <xs:element name="siteprop">
    	<xs:complexType>
    		<xs:attribute name="propname" type="xs:string" use="required"/>
    	</xs:complexType>
    </xs:element>

</xs:schema>
